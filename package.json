{
  "name": "flutter-localization-generator",
  "displayName": "Flutter Localization Generator",
  "description": "Flutter Localization made easy. ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:flutter-localization-generator.create"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "flutter-localization-generator.create",
      "title": "localize_it: create"
    }],
    "menus": {
      "explorer/context": [{
          "command": "flutter-localization-generator.create"
    }]
  }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.72.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
